# -*- bash -*-
# ~/.bash_osx

# Attempt to load brew - bash completions
if [ -f "$(brew --prefix)/etc/bash_completion" ]; then
    source "$(brew --prefix)/etc/bash_completion"
fi

# # Homebrew's sbin fix
# export PATH="/usr/local/sbin:$PATH"

# Travis completion
if [ -f ~/.travis/travis.sh ]; then
    source ~/.travis/travis.sh
fi

# Pyenv Bullshit
eval "$(pyenv init -)"

### Exports
export CLICOLOR=YES
